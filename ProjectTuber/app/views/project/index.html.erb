<%if @projects%>
<% @projects.each do |project| %>
    <div class="row">
      <div class="col s12">
       <h2>
         <%= project.title %>
       </h2>
      </div>
      <div class="col s12">
        <strong class="larger_font">
          "<%= project.description %>"
        </strong>
      </div>
      <div class="col s6">
        <a class="repo_link" href="<%= project.link_to_source %>">&ltCOde&gt</a>
      </div>
    </div>
<% end %>
    <%= will_paginate @projects %>
<%else%>
<h2>No projects yet</h2>
<%end%>